{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "/index.html",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.todobuddy.site"
    }
  ]
}
